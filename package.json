{
  "name": "interactive-comment-section",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "generate": "concurrently \"npm run generate:nexus\" \"npm run generate:genql\"",
    "generate:nexus": "nodemon --exec 'ts-node --transpile-only -P nexus.tsconfig.json src/pages/api/graphql' --ext 'ts' --watch '*/graphql/**/*.ts'",
    "generate:genql": "nodemon --exec 'genql --schema src/graphql/schema.graphql --output src/graphql/generated/genql'  --watch 'graphql/schema.graphql'"
  },
  "dependencies": {
    "@genql/runtime": "2.8.0",
    "@prisma/client": "3.7.0",
    "apollo-server-micro": "^3.5.0",
    "graphql": "^16.2.0",
    "graphql-scalars": "^1.14.1",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "12.0.7",
    "nexus": "^1.2.0-next.17",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.34.6"
  },
  "devDependencies": {
    "@genql/cli": "^2.8.0",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "17.0.5",
    "@types/react": "17.0.38",
    "concurrently": "^6.5.1",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "nodemon": "^2.0.15",
    "prisma": "^3.7.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "4.5.4"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
